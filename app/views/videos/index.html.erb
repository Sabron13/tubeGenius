<% Video.all.each do |video| %>
  <p>
    <p><%= link_to image_tag(thumbnail_url(video.url), size: "200x200"), video_path(video) %></p>
    <%= link_to video.title, video_path(video) %><br>
    <a href="#" data-reveal-id="user-modal"><%= image_tag video.user.gravatar_url(:size => 20) %></a> <%=video.user.name %>
  </p>
<% end %>